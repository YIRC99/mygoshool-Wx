.mymore {
  -webkit-tap-highlight-color: transparent;
  -webkit-user-select: none;
          user-select: none;
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  top: 25rpx;
  right: 40rpx;
}
.mymore image {
  width: 100%;
  height: 100%;
}
.my-car-box {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.my-car-box .car-left {
  display: flex;
  align-items: center;
  margin: 15rpx 0;
  font-size: 32rpx;
}
.my-car-box .car-left text {
  width: 75%;
}
.my-car-box .car-left .my-icon {
  width: 50rpx;
  height: 50rpx;
}
.my-car-box .car-right {
  margin-right: 20rpx;
  margin: 15rpx 0;
  display: flex;
  align-items: center;
}
.my-car-box .car-right image {
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
  overflow: hidden;
  margin-left: 10rpx;
}

