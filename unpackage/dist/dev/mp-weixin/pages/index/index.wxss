  margin-right: 20rpx;
  width: 50rpx;
  height: 50rpx;
}
.login-img-a {
  position: absolute;
  width: 100%;
  height: 450rpx;
  background-size: 100%;
  z-index: -1;
}


 .uni-easyinput__content-input {
  padding-left: 0 !important;
}
.txt {
  font-size: 34rpx;
  font-weight: bold;
  color: #333333;
}
.reg {
  font-size: 28rpx;
  color: #fff;
  height: 90rpx;
  line-height: 90rpx;
  border-radius: 50rpx;
  font-weight: bold;
  background: #f5f6fa;
  color: #000000;
