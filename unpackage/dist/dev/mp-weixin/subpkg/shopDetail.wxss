.WxBox {
  display: flex;
  align-items: center;
  font-size: 36rpx;
}
.WxBox .myicon {
  width: 60rpx;
  height: 60rpx;
  margin-right: 30rpx;
}
.priceAndbrowse {
  display: flex;
  justify-content: space-between;
  padding-bottom: 20rpx;
}
.priceAndbrowse .price {
  font-size: 40rpx;
  font-weight: bold;
  color: #F26666;
}
.myheaderbox {
  display: flex;
  align-items: center;
}
.myheaderbox .left {
  margin-right: 30rpx;
}
.myheaderbox .left .myavatar {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}
.myheaderbox .right .name {
  font-size: 36rpx;
  font-weight: bold;
}
.myheaderbox .right .sub {
  font-size: 28rpx;
  color: #5f6466;
}

